{
    "name": "simplethings/form-serializer-bundle",
    "type": "symfony-bundle",
    "description": "(De-)Serialization of Object-Graphs into XML/JSON based on Form Types",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Eberlei",
            "email": "kontakt@beberlei.de"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.3",
        "symfony/serializer": "~2.3",
        "symfony/form": "~2.3"
    },
    "require-dev": {
        "jms/serializer": "~0.16"
    },
    "suggest": {
        "jms/serializer": ""
    },
    "autoload": {
        "psr-4": {"SimpleThings\\FormSerializerBundle\\": ""}
    },
    "autoload-dev": {
        "psr-4": {"SimpleThings\\FormSerializerBundle\\Tests\\": "Tests/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
